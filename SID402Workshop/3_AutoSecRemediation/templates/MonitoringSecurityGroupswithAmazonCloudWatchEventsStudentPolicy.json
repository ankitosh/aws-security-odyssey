{
    "Version": "2012-10-17",
    "Statement": [
    {
        "Effect":"Allow",
        "Action":[
            "cloudtrail:*",
            "cloudwatch:*",
            "ec2:Describe*",
            "ec2:AuthorizeSecurityGroupIngress",
            "events:*",
            "iam:AddRoleToInstanceProfile",
            "iam:CreateInstanceProfile",
            "iam:Get*",
            "iam:PassRole",
            "iam:List*",
            "kinesis:*",
            "s3:*",
            "sns:*",
            "tag:Get*",
            "logs:*",
            "lambda:*",
            "cloudformation:*",
            "vpc:*"
        ],
        "Resource":"*"
    },
    {
        "Effect": "Deny",
        "Action": [
            "ec2:RunInstances"
        ],
        "Resource": "arn:aws:ec2:*:*:instance/*"
    }
    ]
}
